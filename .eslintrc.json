{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["@typescript-eslint"],
  "root": true,
  "parser": "@typescript-eslint/parser"
}
